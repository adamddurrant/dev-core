{
  "name": "example_theme",
  "version": "0.0.0",
  "description": "An example theme that can be updated as required.",
  "main": "index.js",
  "scripts": {
    "dev": "npm run development",
    "development": "mkdirp dist/build && cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "watch": "npm run development -- --watch",
    "hot": "cross-env NODE_ENV=development mkdirp dist/build && node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js",
    "prod": "npm run production",
    "production": "mkdirp dist/build && cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "browser-sync": "^2.26.7",
    "browser-sync-webpack-plugin": "^2.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.1.1",
    "cross-env": "^6.0.3",
    "eslint": "^6.8.0",
    "eslint-loader": "^3.0.4",
    "eslint-plugin-vue": "^6.2.2",
    "git-describe": "^4.0.4",
    "html-webpack-plugin": "^3.2.0",
    "imagemin-webpack-plugin": "^2.4.2",
    "laravel-mix": "^5.0.4",
    "laravel-mix-clean": "^0.1.0",
    "laravel-mix-eslint": "^0.1.3",
    "laravel-mix-imagemin": "^1.0.3",
    "laravel-mix-string-replace": "^0.1.0",
    "laravel-mix-versionhash": "^1.1.3",
    "mkdirp": "^0.5.5",
    "pugpig-mixphp-npm": "git+https://bitbucket.org/kaldorgroup/pugpig-mixphp-npm.git",
    "pugpig-mixutils-npm": "git+https://bitbucket.org/kaldorgroup/pugpig-mixutils-npm.git",
    "resolve-url-loader": "^3.1.1",
    "sass": "^1.26.8",
    "sass-loader": "^8.0.2",
    "string-replace-loader": "^2.3.0",
    "tar": "^5.0.5",
    "vue-template-compiler": "^2.6.11"
  },
  "repository": {
    "type": "git",
    "url": "git://bitbucket.org/kaldorgroup/pugpig-wordpress-template.git"
  }
}
